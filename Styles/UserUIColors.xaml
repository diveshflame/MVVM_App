<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


        <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#F5F5F5</Color>
    <Color x:Key="primaryBackColor2">#E0E0E0</Color>
    <Color x:Key="secondaryBackColor1">#009688</Color>
    <Color x:Key="secondaryBackColor2">#26A69A</Color>

